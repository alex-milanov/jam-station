{
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"extends": "google",
	"rules": {
		"indent": ["error", "tab", {"SwitchCase": 1}],
		"max-len": ["warn", 120, {"tabWidth": 2}],
		"no-nested-ternary": 0,
		"operator-linebreak": ["error", "after", { "overrides": { "?": "before", ":": "before", "&&": "before", "||": "before" } }],
		"no-return-assign": ["error", "except-parens"],
		"no-sequences": 0,
		"dot-notation": 0,
		"no-negated-condition": 0,
		"no-unused-vars": 0
	}
}
