{
  "name": "jam-station",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:assets && npm run build:js && npm run build:sass",
    "build:js": "browserify src/js/index.js -o dist/js/app.js --debug",
    "build:sass": "node-sass --source-map dist/css/style.css.map --include-path=$(node bin/sass-paths.js) src/sass/style.sass dist/css/style.css",
    "build:assets": "node bin/move-assets.js",
    "watch": "npm run watch:js & npm run watch:sass & npm run watch:assets & npm run livereload",
    "watch:js": "watchify -p browserify-hmr src/js/index.js -o dist/js/app.js --debug",
    "watch:sass": "nodemon -w src/sass -e sass -x 'npm run build:sass'",
    "watch:assets": "nodemon -e jpg,jpeg,gif,png,json,md --watch src/assets --exec 'npm run build:assets'",
    "livereload": "livereload dist/**/*.css -d",
    "start": "npm run build && serve --path dist & npm run watch",
    "commit": "npm run build && git add . && git commit && git push && npm run sync:gh"
  },
  "author": "Alex Milanov <alex@i4web.biz>",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.5",
    "eslint": "^3.3.1",
    "eslint-config-google": "^0.6.0",
    "fs-extra": "^0.30.0",
    "gulp": "^3.9.1",
    "livereload": "^0.5.0",
    "node-sass": "^3.13.1",
    "node-serve": "0.0.3",
    "nodemon": "^1.10.0",
    "watchify": "^3.8.0"
  },
  "dependencies": {
    "bourbon": "^4.2.7",
    "bourbon-neat": "^1.8.0",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "iblokz-data": "^1.2.0",
    "iblokz-snabbdom-helpers": "^1.1.0",
    "jszip": "^3.1.3",
    "midi-clock": "0.0.1",
    "moment": "^2.17.1",
    "osc": "^2.2.4",
    "raf-stream": "^3.0.0",
    "rx": "^4.1.0",
    "rx-node": "^1.0.2",
    "superagent": "^3.3.1",
    "tap-tempo": "^0.1.1",
    "uid-safe": "^2.1.3",
    "vex-dialog": "^1.1.0",
    "vex-js": "^3.1.0",
    "ws": "^6.0.0"
  }
}
